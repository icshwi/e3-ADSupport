EPICS_MODULE_NAME:=ADSupport
# This is the module source tag
# *) individual tags
#    export EPICS_MODULE_TAG:=tags/1.1.1
# *) master branch 
#    export EPICS_MODULE_TAG:=master
# *) individual hash 
export EPICS_MODULE_TAG:=tags/R1-4
export EPICS_MODULE_SRC_PATH:=ADSupport
export ESS_MODULE_MAKEFILE:=$(EPICS_MODULE_NAME).Makefile
export PROJECT:=$(EPICS_MODULE_NAME)
export LIBVERSION:=1.4.0
export E3_ENV_PATH:=e3-env
export E3_ENV_NAME:=e3-env
#export E3_ENV_NAME:=E3_ENV_CONF
#export E3_ENV_TAG:=v0.1

#ADSupport module configuration
export WITH_NETCDF   	   = YES
export WITH_JPEG     	   = NO
export WITH_ZLIB     	   = NO
export WITH_TIFF     	   = NO
export WITH_SZIP     	   = NO
#Including XML2 introduce some conflicts with other module!! Try to use the linux one.
export WITH_XML2	 	   = NO
export WITH_HDF5     	   = NO
export WITH_NEXUS          = NO

#GraphicsMagick is partially integrated. It generate a lot of name conflicts. It is disabled since not required currently.
export WITH_GRAPHICSMAGICK = NO



